<p id="notice"><%= notice %></p>

<h1>Listing Cuenta</h1>

<table>
  <thead>
    <tr>
      <th>Cuenta</th>
      <th>Subcuenta1</th>
      <th>Subcuenta2</th>
      <th>Subcuenta3</th>
      <th>Subcuenta4</th>
      <th>Subcuenta5</th>
      <th>Descripcion</th>
      <th>Nombre</th>
      <th>Estado</th>
      <th>Tipo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cuenta.each do |cuentum| %>
      <tr>
        <td><%= cuentum.cuenta %></td>
        <td><%= cuentum.subcuenta1 %></td>
        <td><%= cuentum.subcuenta2 %></td>
        <td><%= cuentum.subcuenta3 %></td>
        <td><%= cuentum.subcuenta4 %></td>
        <td><%= cuentum.subcuenta5 %></td>
        <td><%= cuentum.descripcion %></td>
        <td><%= cuentum.nombre %></td>
        <td><%= cuentum.estado %></td>
        <td><%= cuentum.tipo %></td>
        <td><%= link_to 'Show', cuentum %></td>
        <td><%= link_to 'Edit', edit_cuentum_path(cuentum) %></td>
        <td><%= link_to 'Destroy', cuentum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cuenta', new_cuentum_path %>
